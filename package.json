{
  "name": "n8n-nodes-touchbasepro",
  "version": "0.1.0",
  "description": "Official Touchbase Pro Node for n8n",
  "keywords": [
    "touchbasepro",
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "main": "index.js",
  "files": [
    "dist",
    "index.js",
    "LICENSE.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/touchbasepro/n8n-nodes-touchbasepro.git"
  },
  "author": {
    "name": "Ysrajsingh",
    "email": "hello@yashrajs.com"
  },
  "bugs": {
    "url": "https://github.com/touchbasepro/n8n-nodes-touchbasepro/issues"
  },
  "homepage": "https://www.touchbasepro.io/",
  "engines": {
    "node": ">=20.15"
  },
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TouchBaseProApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TouchBasePro/TouchBasePro.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}